{
  "name": "core-components-angular-lib",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run bundle-styles",
    "bundle-styles": "scss-bundle -c scss-bundle.config.json",
    "build:watch": "ng build --watch",
    "e2e": "ng e2e",
    "lint": "node_modules/.bin/tslint --project . && prettier -l \"projects/**/*.ts\"",
    "lint-fix": "node_modules/.bin/tslint --fix --project . && prettier -l \"projects/**/*.ts\" --write",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006 -s src/assets",
    "build-storybook": "npm run build && npm run docs:json && build-storybook",
    "test": "npm run lint & ng test --watch=false --browsers=HeadlessChrome --codeCoverage",
    "code-coverage": "ng test --codeCoverage=true --watch=false --browsers=HeadlessChrome && http-server -c-1 -o -p 9875 ./coverage",
    "test-chrome": "ng test --watch=false --codeCoverage=true --browsers=localChrome",
    "test-chrome-watch": "ng test --watch=true --browsers=localChrome",
    "test-chrome-headless": "ng test --watch=false --codeCoverage=true --browsers=HeadlessChrome",
    "test-chrome-headless-watch": "ng test --watch=true --browsers=HeadlessChrome"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@types/node": "^15.0.1",
    "@villemontreal/boite-outils4-web": "^4.10.0",
    "css-loader": "^3.5.3",
    "karma-spec-reporter": "0.0.32",
    "react": "^16.13.1",
    "react-is": "^16.13.1",
    "rxjs": "~6.4.0",
    "scss-bundle": "^3.1.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "~8.3.21",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@babel/core": "^7.9.6",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^6.2.9",
    "@storybook/angular": "^6.2.9",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@villemontreal/lint-config-villemontreal": "^1.6.0",
    "babel-loader": "^8.1.0",
    "codelyzer": "^5.0.0",
    "commitlint": "^11.0.0",
    "husky": "^3.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-junit-reporter": "^1.2.0",
    "ng-packagr": "^5.0.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.19.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.0"
  }
}
